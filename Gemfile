source 'https://rubygems.org'

gem 'rails', '~> 4.1.x'

gem 'aws-sdk'
gem 'coffee-rails'
gem 'jquery-rails'
gem 'paperclip'
gem 'mysql2'
gem 'sass-rails'
gem 'uglifier'

group :development, :test do
  gem 'rspec-rails'
  gem 'sqlite3'
  gem 'launchy'
end

group :test do
  gem 'capybara'
  gem 'shoulda-matchers'
end

group :development do
  # Use Capistrano to automate deployments.
  gem 'capistrano'
  gem 'capistrano-rails'
  gem 'capistrano-rvm'
  gem 'cap-ec2'
end
